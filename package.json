{
  "name": "jsonschema-ts-type",
  "version": "0.0.0-semantic-release",
  "description": "TypeScript Type Helper to convert JSONSchema to TypeScript Type",
  "types": "src/index.ts",
  "scripts": {
    "commit": "cz",
    "test": "tsc",
    "lint": "eslint \"src/**/*.ts\"",
    "watch": "tsc --watch",
    "prepare": "husky install"
  },
  "homepage": "https://github.com/YuJianrong/jsonschema-ts-type",
  "bugs": {
    "url": "https://github.com/YuJianrong/jsonschema-ts-type/issues"
  },
  "author": "Jianrong Yu <yujianrong@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@types/node": "^15.6.1",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "commitizen": "^4.2.5",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.0",
    "typescript": "^4.2.4"
  },
  "lint-staged": {
    "*.ts": "eslint --cache --max-warnings 0 --fix"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
